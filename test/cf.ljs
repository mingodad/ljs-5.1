// temperature conversion table (celsius to farenheit)

for( c0=-20,50-1,10 ) {
	io.write("C ");
	for( c=c0,c0+10-1 ) {
		io.write(string.format("%3.0f ",c));
	}
	io.write("\n");
	
	io.write("F ");
	for( c=c0,c0+10-1 ) {
		f=(9/5)*c+32;
		io.write(string.format("%3.0f ",f));
	}
	io.write("\n\n");
}
